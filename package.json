{
  "name": "projects-library",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "add-dist-subtree": "git add dist && git commit -m 'adding dist subtree'",
    "deploy-to-ghpages": "git subtree push --prefix dist origin gh-pages",
    "github-pages": "yarn build && yarn add-dist-subtree && yarn deploy-to-ghpages"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "lodash": "^4.17.21",
    "vue": "^2.6.14",
    "vue-lodash": "^2.1.2",
    "vue-router": "3.5.1",
    "vuetify": "^2.6.0",
    "vue-moment": "^4.1.0",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
